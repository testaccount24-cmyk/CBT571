//RECVPDS  JOB 'DEMO DSORG=PO',CLASS=A,MSGCLASS=A
//*
//* TRANSMIT AND RECEIVE PDS, ZAP DUMP
//*
//DELETE   EXEC PGM=IEHPROGM
//SYSPRINT  DD SYSOUT=*
//MVS3380   DD UNIT=3380,VOL=SER=MVS809,DISP=SHR
//SYSIN     DD *
 SCRATCH DSNAME=SYS2.XMIT370.DEMO.XMIT,VOL=3380=MVS809
 UNCATLG DSNAME=SYS2.XMIT370.DEMO.XMIT
 SCRATCH DSNAME=SYS2.XMIT370.IEBCOPY.DATA,VOL=3380=MVS809
 UNCATLG DSNAME=SYS2.XMIT370.IEBCOPY.DATA
 SCRATCH DSNAME=SYS2.RECV370.DEMO.PDS,VOL=3380=MVS809
 UNCATLG DSNAME=SYS2.RECV370.DEMO.PDS
//*
//XMIT370  EXEC PGM=XMIT370
//STEPLIB   DD DSN=SYS2.LOCAL.LINKLIB,DISP=SHR
//XMITDBUG  DD DSN=SYS2.XMIT370.ASM(KBUGXMIT),DISP=SHR
//XMITLOG   DD SYSOUT=*
//SYSPRINT  DD SYSOUT=*
//SYSUT1    DD DSN=SYS2.XMIT370.ASM,DISP=SHR
//SYSUT2    DD DSN=&&SYSUT2,
//             UNIT=SYSDA,                            SYSALLDA
//             SPACE=(CYL,(5,1)),
//             DISP=(,DELETE,DELETE)
//XMITOUT   DD DSN=SYS2.XMIT370.DEMO.XMIT,
//             UNIT=3380,VOL=SER=MVS809,
//             SPACE=(CYL,(5,1)),
//             DISP=(,CATLG)
//SYSIN     DD DUMMY
//SYSUDUMP  DD SYSOUT=*
//COPYR1    DD DUMMY
//*
//RECV370  EXEC PGM=RECV370
//STEPLIB   DD DSN=SYS2.LOCAL.LINKLIB,DISP=SHR
//RECVDBUG  DD DSN=SYS2.XMIT370.ASM(KBUGRECV),DISP=SHR
//RECVLOG   DD SYSOUT=*
//XMITIN    DD DSN=SYS2.XMIT370.DEMO.XMIT,DISP=SHR
//SYSPRINT  DD SYSOUT=*
//SYSUT1    DD DSN=SYS2.XMIT370.IEBCOPY.DATA,
//             UNIT=SYSDA,VOL=SER=MVS809,
//             SPACE=(CYL,(5,1)),
//             DISP=(,CATLG,CATLG)
//SYSUT2    DD DSN=SYS2.RECV370.DEMO.PDS,
//             UNIT=3380,VOL=SER=MVS809,
//             SPACE=(CYL,(5,1,45),RLSE),
//             DISP=(,CATLG,DELETE)
//SYSIN     DD DUMMY
//SYSUDUMP  DD SYSOUT=*
//*
//ZAP      EXEC PGM=IKJEFT01,REGION=1024K,DYNAMNBR=20,COND=EVEN
//SYSPRINT  DD SYSOUT=*
//SYSHELP   DD DISP=SHR,DSN=SYS1.HELP
//SYSTSPRT  DD SYSOUT=*
//ZAPLOG    DD SYSOUT=*
//SYSTSIN   DD *
ZAP 'SYS2.XMIT370.IEBCOPY.DATA' NOT3270 LOG VERBOSE
DUMP
END
//
